{
  "manifest_version": 2,
  "name": "SoundCloud Player",
  "short_name": "SC Player",
  "description": "Popup Player",
  "version": "1.2.8",
  "homepage_url": "https://github.com/S4WA/soundcloud-player",
  "permissions": [
    "tabs",
    "clipboardRead"
  ],
  "content_scripts": [ {
    "all_frames": true,
    "js": [
      "contents/contents.js",
      "contents/utils.js",
      "jquery-3.4.1.min.js"
    ],
    "matches": [
      "*://soundcloud.com/*"
    ],
    "run_at": "document_end"
  } ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}